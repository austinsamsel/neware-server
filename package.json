{
  "name": "neware-server",
  "version": "1.0.0",
  "description": "server layer for neware",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -w index.js --exec \"babel-node index.js --presets es2015,stage-0\"",
    "old-start": "node --harmony-async-await index",
    "deploy": "now -e GREETING=@greeting -e FIREBASE_ID=@firebase_id -e FIREBASE_EMAIL=@firebase_email -e FIREBASE_KEY=@firebase_key -e FIREBASE_URL=@firebase_url",
    "ship": "now -e GREETING=@greeting -e FIREBASE_ID=@firebase_id -e FIREBASE_EMAIL=@firebase_email -e FIREBASE_URL=@firebase_url",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "dist": "node dist"
  },
  "author": "hightops",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "firebase-admin": "^4.1.0",
    "micro": "^6.2.1"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "nodemon": "^1.11.0"
  }
}
